# vim: ft=sh
namecase="font-util"
name="xorg-font-util"
iteration="1"
version="1.3.0"
desc="X.Org font utilities"
bdeps="xorg-util-macros"
deps=""
arch="any"
flags=""
sdir="font"


function configure() {
	./configure \
		--prefix=/usr \
		--mandir=/usr/share/man \
		--with-mapdir=/usr/share/fonts/util \
		--with-fontrootdir=/usr/share/fonts 
}
#config default
#build default
#installpkg repo default
