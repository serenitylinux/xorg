# vim: ft=sh
name="libfontenc"
iteration="1"
version="1.1.2"
desc="X11 font encoding library"
bdeps="zlib pkg-config xproto"
deps="zlib"
arch="any"
flags=""
sdir="lib"


function configure() {
  ./configure --prefix=/usr --sysconfdir=/etc \
      --localstatedir=/var --disable-static \
      --with-encodingsdir=/usr/share/fonts/encodings 
}
#build default
#installpkg repo default
