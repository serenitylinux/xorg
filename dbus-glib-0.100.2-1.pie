# vim: ft=sh
name="dbus-glib"
iteration="1"
version="0.100.2"
desc="Glib bindings for DBus"
bdeps="dbus glib2 pkg-config"
deps="dbus glib2"
arch="any"
flags=""
src="http://dbus.freedesktop.org/releases/$name/$name-$version.tar.gz"

function configure() {
	./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var --enable-static=no --enable-bash-completion=no
}
